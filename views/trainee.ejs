<!DOCTYPE html>
<html class="no-js" lang="en" ng-app="DVPortalApp">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title><%= title %></title>
    <meta name="description" content="">
    <meta name="author" content="Chrisnankam24">

    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Place favicon.ico in the root directory -->

    <link rel="stylesheet" href="/css/normalize.css">

    <link rel="stylesheet" href="/js/vendor/fullpage/jquery.fullpage.min.css">
    <link rel="stylesheet" href="/semantic/dist/semantic.min.css">
    <link rel="stylesheet" href="/js/vendor/flatpickr/flatpickr.min.css">

    <link rel="stylesheet" href="/css/style.css">

    <!--[if lt IE 9]>
    <script src="/js/vendor/html5.js"></script>
    <![endif]-->

    <!--Import angular js script-->
    <script src="/js/vendor/angular/angular.min.js"></script>

</head>

<body ng-controller="clientController">

<!--Include Trainee / Manager Header-->

<%- include('headers/trainee-header', params); %>

<div class="ui active tab" data-tab="training">

</div>


<div class="ui tab" data-tab="quiz">

</div>

<div class="ui tab" data-tab="resources">

</div>

<% if (params.is_manager || params.is_trainer) {%>
    <div class="ui tab" data-tab="team-management">

    </div>
<%}%>

<div class="ui center aligned container" style="margin-top: 40px;">
    <div class="ui stackable inverted divided grid">

        <!--Include Trainee / Manager Menu and Calendar view. This is the first column of the trainee / manager UI-->

        <%- include('content/trainee-menu-calendar', params); %>

        <!--Include Trainee / Manager Main Content view. This is the second column of the trainee / manager UI-->

        <%- include('content/trainee-main-content', params); %>

        <!--Include Trainee / Manager stats and feedback view. This is the third column of the trainee / manager UI-->

        <%- include('content/trainee-stats-feedback', params); %>

    </div>
</div>

<!--Include UI Modals-->

<%- include('modals/modals', params); %>

<!-- REQUIRED JS SCRIPTS -->
<script src="/js/vendor/jQuery/jQuery-2.1.4.min.js"></script>
<script src="/js/vendor/fullpage/jquery.fullpage.min.js"></script>
