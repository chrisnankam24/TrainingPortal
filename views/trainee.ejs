<script src="/js/vendor/flatpickr/flatpickr.min.js"></script>
<script src="/js/vendor/date/date.js"></script>
<script src="/js/vendor/tinymce/tinymce.min.js"></script>
<script src="/js/vendor/initial/initial.min.js"></script>
<script src="/js/vendor/tablesort/tablesort.js"></script>
<script src="/js/vendor/lory/lory.min.js"></script>
<script src="/js/vendor/crossroads/signals.min.js"></script>
<script src="/js/vendor/hasher/hasher.min.js"></script>
<script src="/js/vendor/crossroads/crossroads.min.js"></script>
<script src="/js/vendor/time-ago/timeago.js"></script>
<script src="/semantic/dist/semantic.min.js"></script>
<script src="/js/initialization.js"></script>
<script src="/js/main.js"></script>
<script src="/js/app.js"></script>
<script src="/js/controllers/clientController.js"></script>
<script src="/js/controllers/headerController.js"></script>
<script src="/js/controllers/groupController.js"></script>
<script src="/js/controllers/evaluationFormController.js"></script>
<script src="/js/controllers/quizFormController.js"></script>
