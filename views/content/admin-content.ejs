                                                                                   tabindex="0">
                                                                            <div class="default text">Select
                                                                                Resource Visibility
                                                                            </div>
                                                                            <div class="menu">
                                                                                <div class="item"
                                                                                     data-value="0">
                                                                                    Public
                                                                                </div>
                                                                                <div class="item"
                                                                                     data-value="1">
                                                                                    Private
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <input type="hidden" name="" id="resource_id" value="{{resource_id}}">
                                                                <div class="ui container">
                                                                    <div class="ui submit right floated primary mini button " ng-click="addModifyResource()">
                                                                        {{addModifText}} Resource
                                                                    </div>
                                                                    <div class="ui submit right floated mini button" ng-click="cancelResourceModification()">
                                                                        Cancel
                                                                    </div>
                                                                </div>
                                                            </form>
                                                        </div>
                                                    </div>
                                                    <div class="ui grid container"
                                                         style="padding: 14px 14px 0px 0px; margin-bottom: 0px;">
                                                        <div class="one wide column" style="padding-left: 10px;">

                                                        </div>

                                                        <div class="eleven wide column"
                                                             style="padding: 10px 14px 6px;">
                                                            <div class="ui mini icon buttons"
                                                                 style="margin-left: 90px;">
                                                                <button class="ui button" ng-disabled="DISABLE_BACKWARD_NAV" ng-click="loadPrev()"><i class="chevron left icon"></i></button>
                                                                <button class="ui button">
                                                                    <span>{{resources_list_min}}</span>
                                                                    <span>-</span>
                                                                    <span>{{resources_list_max}}</span>
                                                                    <span>of</span>
                                                                    <span>{{resources_list_total}}</span>
                                                                </button>
                                                                <button class="ui button" ng-disabled="DISABLE_FORWARD_NAV" ng-click="loadNext()"><i class="chevron right icon"></i></button>
                                                            </div>
                                                        </div>
                                                        <div class="four wide column"
                                                             style="padding: 10px 10px 6px 14px;">
                                                            <div class="ui mini buttons" style="float: right;">
                                                                <button class="ui mini primary button" style="padding-right: 5px; padding-left: 10px;" ng-click="startAddResource()"><i
                                                                            class="plus icon"></i>
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div id="resource-table" style="margin-left: 5px; margin-right: 5px;">
                                                        <table class="ui very basic left small selectable aligned table sort"
                                                               id="resource-table" style="font-size: 11px; margin-top: 5px; display: block; overflow-x: auto; height: 425px;">
                                                            <thead>
                                                            <tr class="center aligned">
                                                                <th class="sort-header">ID</th>
                                                                <th class="sort-header">Resource</th>
                                                                <th class="sort-header">Type</th>
                                                                <th class="sort-header">Link</th>
                                                                <th class="sort-header">Visibility</th>
                                                                <th class="sort-header">No Downloads</th>
                                                                <th class="sort-header">Addition Date</th>
                                                                <th></th>
                                                            </tr>
                                                            </thead>
                                                            <tbody>
                                                            <tr class="center aligned" ng-repeat="resource in resources_list">
                                                                <!--<td ng-bind="resource.resourceID"></td>-->
                                                                <td ng-bind="$index + resources_list_min"></td>
                                                                <td ng-bind="resource.resource_name"></td>
                                                                <td ng-bind="resource.resourceType"></td>
                                                                <td ng-bind="resource.link"></td>
                                                                <td ng-bind="resource.resourceVisibility == 0 ? 'Public' : 'Private'"></td>
                                                                <td ng-bind="resource.num_downloads"></td>
                                                                <td ng-bind="resource.addition_date | date : 'EEE dd, MMM, yyyy'"></td>
                                                                <td>
                                                                    <div class="ui icon mini buttons">
                                                                        <button class="ui button" ng-click="startModifyResource(resource)"><i
                                                                                    class="write link icon"></i>
                                                                        </button>
                                                                        <button class="ui grey button" ng-click="deleteResource(resource)"><i
                                                                                    class="remove link icon"></i>
                                                                        </button>
                                                                    </div>
                                                                </td>
                                                            </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
